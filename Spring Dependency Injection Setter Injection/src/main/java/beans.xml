<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.rammohan.spring.dependency.injection.Person" id="rmr">
		<property name="myName" value="Ram Mohan Reddy" />
		<property name="id" value="150" />
	</bean>
	
	<bean class="com.rammohan.spring.dependency.injection.Person" id="sravan">
		<property name="myName" value="Sravan Dwadasi" />
		<property name="id" value="117" />
	</bean>
</beans>